package test_1;

public class test_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int dan=1; dan <10; dan++) {
			System.out.println(dan + "´Ü");
				for(int num=1; num <10; num++) {	
					if(dan >= num) {
						int total = dan * num;
						System.out.printf("%2d" + "x" + num + "=" + total, dan);
						}
				}
				System.out.println();
			}
	}
}
