package test_1;

public class test_4 {
	public static void main(String[] args) {
		
		for (int row=1; row<=4; row++) {
			for(int col=1; col<=4-row; col++) {
				System.out.print("1");
			}
			/*
			 * for(int col=1; co);{
			 * 
			 * }
			 */			
			for(int col=1; col>=4+row; col++) {
				System.out.print("2");
				
			}
			System.out.println();
		}
				
	}

}
