package test_1;

public class java_test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age = 10;
		
		if (age > 8) {
			System.out.println("학교에 다닙니다.");
		}
		else {
			System.out.println("학교에 다니지 않습니다.");
		}
		
		System.out.print("나는 " + age + "살 입니다");
	}

}
