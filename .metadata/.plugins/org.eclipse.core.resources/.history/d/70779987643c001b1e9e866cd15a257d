package test_1;

public class java_test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int rank = 1;
		char medalColor = ' ';
		
		switch(rank) {
			case 1 : medalColor = 'G';
				break;
			case 10 : medalColor = 'D';
				break;
		}
		
		System.out.println("매달색은" + medalColor + "입니다");
		
	}

}
